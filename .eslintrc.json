{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
	"settings": {
		"react": {
			"version": "16.13.1"
		}
	},
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
		"react/no-unescaped-entities": 0,
		"react/display-name": 0
	}
}
