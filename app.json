{
    "expo": {
        "name": "JAK-Mobile-App",
        "slug": "JAK-Mobile-App",
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.beastnighttv.jakmobileapp"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.beastnighttv.jakmobileapp"
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        }
    }
}
