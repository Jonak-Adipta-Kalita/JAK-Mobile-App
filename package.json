{
    "name": "jak-mobile-app",
    "description": "A Mobile App made by JAK",
    "author": "Jonak Adipta Kalita",
    "license": "MIT",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "dev": "expo start --dev-client",
        "test": "jest --passWithNoTests",
        "start:android": "expo start --android",
        "start:ios": "expo start --ios",
        "eject": "expo eject",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prettier": "prettier **/*.{js,jsx,tsx,ts,md,json}",
        "prettier:fix": "prettier --write **/*.{js,jsx,tsx,ts,md,json}",
        "format": "eslint --fix . && prettier --write **/*.{js,jsx,tsx,ts,md,json}",
        "build": "eas build -p android && eas build -p ios",
        "build:android": "eas build -p android",
        "build:ios": "eas build -p ios",
        "publish": "firebase deploy && eas update",
        "publish:firebase": "firebase deploy",
        "publish:expo": "eas update",
        "check:version": "ncu",
        "prepare": "husky install"
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
        ]
    },
    "bugs": {
        "url": "https://github.com/Jonak-Adipta-Kalita/JAK-Mobile-App/issues"
    },
    "homepage": "https://github.com/Jonak-Adipta-Kalita/JAK-Mobile-App#readme",
    "dependencies": {
        "@react-native-async-storage/async-storage": "1.17.11",
        "@react-native-masked-view/masked-view": "0.2.8",
        "@react-navigation/drawer": "^6.6.2",
        "@react-navigation/material-bottom-tabs": "^6.2.15",
        "@react-navigation/material-top-tabs": "^6.6.2",
        "@react-navigation/native": "^6.1.6",
        "@react-navigation/stack": "^6.3.16",
        "@reduxjs/toolkit": "^1.9.3",
        "@rneui/base": "^4.0.0-rc.7",
        "@rneui/themed": "^4.0.0-rc.7",
        "@xxjonakadiptaxx/jak_javascript_package": "^3.3.3",
        "expo": "~48.0.9",
        "expo-asset": "~8.9.1",
        "expo-auth-session": "~4.0.3",
        "expo-constants": "~14.2.1",
        "expo-dev-client": "~2.2.1",
        "expo-font": "~11.1.1",
        "expo-image-picker": "~14.1.1",
        "expo-notifications": "~0.18.1",
        "expo-random": "~13.1.1",
        "expo-status-bar": "~1.4.4",
        "expo-updates": "~0.16.4",
        "firebase": "^9.19.1",
        "moment": "^2.29.4",
        "nativewind": "^2.0.11",
        "react": "18.2.0",
        "react-firebase-hooks": "^5.1.1",
        "react-native": "0.71.6",
        "react-native-dotenv": "^3.4.8",
        "react-native-gesture-handler": "~2.9.0",
        "react-native-pager-view": "6.1.2",
        "react-native-paper": "^5.5.2",
        "react-native-reanimated": "~2.14.4",
        "react-native-safe-area-context": "4.5.0",
        "react-native-screens": "~3.20.0",
        "react-native-svg": "13.4.0",
        "react-native-tab-view": "^3.5.1",
        "react-native-vector-icons": "^9.2.0",
        "react-redux": "^8.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@types/jest": "^29.5.0",
        "@types/react": "~18.0.14",
        "@types/react-native": "^0.71.5",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "axios": "^1.3.4",
        "dotenv": "^16.0.3",
        "eas-cli": "^3.8.1",
        "eslint": "^8.37.0",
        "eslint-plugin-react": "^7.32.2",
        "firebase-tools": "^11.25.2",
        "husky": "^8.0.3",
        "jest": "^29.2.1",
        "jest-expo": "^48.0.2",
        "npm-check-updates": "^16.10.0",
        "prettier": "^2.8.7",
        "prettier-plugin-tailwindcss": "^0.2.6",
        "tailwindcss": "^3.3.1",
        "typescript": "^4.9.4"
    },
    "private": true
}
