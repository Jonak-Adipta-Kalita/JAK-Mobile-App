{
    "name": "jak-mobile-app",
    "description": "A Mobile App made by JAK",
    "author": "Jonak Adipta Kalita",
    "license": "MIT",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "test": "jest --passWithNoTests",
        "start:android": "expo start --android",
        "start:ios": "expo start --ios",
        "start:web": "expo start --web",
        "eject": "expo eject",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prettier": "prettier **/*.{js,jsx,tsx,ts,md,json}",
        "prettier:fix": "prettier --write **/*.{js,jsx,tsx,ts,md,json}",
        "format": "eslint --fix . && prettier --write **/*.{js,jsx,tsx,ts,md,json}",
        "build": "expo build:android && expo build:ios && expo build:web",
        "build:android": "expo build:android",
        "build:ios": "expo build:ios",
        "build:web": "expo build:web",
        "publish": "firebase deploy && expo publish",
        "publish:firebase": "firebase deploy",
        "publish:expo": "expo publish",
        "check:version": "ncu",
        "prepare": "husky install"
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.|react-navigation|@react-navigation/.|@unimodules/.|unimodules|sentry-expo|native-base|@sentry/.)"
        ]
    },
    "bugs": {
        "url": "https://github.com/Jonak-Adipta-Kalita/JAK-Mobile-App/issues"
    },
    "homepage": "https://github.com/Jonak-Adipta-Kalita/JAK-Mobile-App#readme",
    "dependencies": {
        "@react-native-async-storage/async-storage": "~1.15.0",
        "@react-native-community/masked-view": "^0.1.11",
        "@react-navigation/drawer": "^6.1.8",
        "@react-navigation/material-bottom-tabs": "^6.0.9",
        "@react-navigation/material-top-tabs": "^6.0.6",
        "@react-navigation/native": "^6.0.6",
        "@react-navigation/stack": "^6.0.11",
        "@reduxjs/toolkit": "^1.8.3",
        "expo": "^43.0.3",
        "expo-asset": "~8.4.3",
        "expo-constants": "~12.1.3",
        "expo-font": "~10.0.3",
        "expo-google-app-auth": "~9.0.0",
        "expo-image-picker": "~11.0.3",
        "expo-notifications": "~0.13.3",
        "expo-status-bar": "~1.1.0",
        "firebase": "^9.6.7",
        "moment": "^2.29.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-firebase-hooks": "^5.0.2",
        "react-native": "0.64.3",
        "react-native-dotenv": "^3.3.0",
        "react-native-elements": "3.4.2",
        "react-native-gesture-handler": "~1.10.2",
        "react-native-pager-view": "5.4.6",
        "react-native-paper": "^4.9.2",
        "react-native-reanimated": "~2.2.0",
        "react-native-safe-area-context": "3.3.2",
        "react-native-screens": "~3.8.0",
        "react-native-svg": "12.1.1",
        "react-native-tab-view": "^3.1.1",
        "react-native-vector-icons": "^9.0.0",
        "react-native-web": "0.17.1",
        "react-redux": "^7.2.6"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^7.32.0",
        "eslint-plugin-react": "^7.28.0",
        "firebase-tools": "^10.9.2",
        "husky": "^7.0.4",
        "jest-expo": "^43.0.1",
        "npm-check-updates": "^12.0.2",
        "prettier": "^2.4.1",
        "typescript": "~4.3.5"
    },
    "private": true
}
